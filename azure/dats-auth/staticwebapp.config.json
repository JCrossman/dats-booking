{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "*.css", "*.js", "*.png", "*.ico"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; connect-src 'self' https://datsonlinebooking.edmonton.ca"
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
