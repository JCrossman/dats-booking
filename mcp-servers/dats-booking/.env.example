# Required: AES-256 encryption key for credential storage
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
DATS_ENCRYPTION_KEY=

# Optional: Logging level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Rate limit between DATS portal requests (milliseconds)
DATS_RATE_LIMIT_MS=3000
